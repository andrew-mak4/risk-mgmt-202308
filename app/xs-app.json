{
  "welcomeFile": "/app/risks/webapp/index.html",
  "routes": [
    {
      "source": "^/app/(.*)$",
      "localDir": "./",
      "target": "$1"
    },
    {
      "source": "^/appconfig/",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/service/(.*)$",
      "destination": "srv-api"
    }
  ]
}